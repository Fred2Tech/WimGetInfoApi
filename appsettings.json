{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "WimAnalyzer": {
    "SupportedExtensions": [ ".wim", ".esd" ],
    "MaxFileSizeGB": 10,
    "UseRealService": true,
    "EnableManagedWimLib": true,
    "CacheEnabled": true
  }
}
